<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Copyright>2020, 2021</Copyright>
    <Company>RazorSoft Media, DBA</Company>
    <Authors>RazorSoft Media</Authors>
    <Configuration>Debug</Configuration>
    <FileVersion>2.0.0.1</FileVersion>
    <InformationalVersion>1.0.0.0</InformationalVersion>
    <AssemblyVersion>2.0.0.1</AssemblyVersion>
    <AssemblyTitle>RazorSoft.Core</AssemblyTitle>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId />
    <Version>2.0.0.1</Version>
    <PackageProjectUrl>https://github.com/RazorSoft-Media/RazorSoft.Core</PackageProjectUrl>
    <Description>Core utilities &amp; helpers:
- Linq extensions
- Generic Base Repository implementation
- ObjectContext interface (generic &amp; non-generic)</Description>
    <RepositoryUrl>https://github.com/RazorSoft-Media/RazorSoft.Core</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReleaseNotes>See Release Notes: https://github.com/RazorSoft-Media/RazorSoft.Core/blob/master/wiki/.documentation/release_notes.md
</PackageReleaseNotes>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageTags>net-50</PackageTags>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>bin\Debug\</OutputPath>
    <DocumentationFile>bin\Debug\RazorSoft.Core.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Infrastructure\**" />
    <EmbeddedResource Remove="Infrastructure\**" />
    <None Remove="Infrastructure\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="Extensions\GeneratorExtensions.cs" />
  </ItemGroup>

</Project>
